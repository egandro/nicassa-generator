<% if(namespace !== undefined && namespace !== null){ %>
export namespace <%- namespace%> {
<% } %><% if(createDecorators){
%>import { SequelizeEntitiy } from './decorators';
<% } %><% if(tables.length>0){ %>
////////////////////////////////////////////////////////////////////
// TABLES
////////////////////////////////////////////////////////////////////
<% } %><%tables.forEach(function(table){%>
/**
 * SEQUELIZE ENTITY
 * table: <%- table.getMappedName('None')%>
 * module: <%- projectName %>
 */
<% if(createDecorators){
%>@SequelizeEntitiy('<%- projectName%>')
<% }
%>export<% if(createDecorators){%> class <% } else {%> interface <% } %><%- table.getMappedName('TypeScript')%> {<%
table.columns.forEach(function(column){%>
    <%- column.getMappedName('TypeScript')%><% if(column.nullable) {%>?<%}%>: <%- column.getMappedDataType('TypeScript');%>;<%
});%>
}
<%}); %>
<% if(views.length>0){ %>
////////////////////////////////////////////////////////////////////
// VIEWS
////////////////////////////////////////////////////////////////////
<% } %><%views.forEach(function(view){%>
/**
 * SEQUELIZE ENTITY
 * view: <%- view.getMappedName('None')%>
 * module: <%- projectName %>
 */
<% if(createDecorators){
%>@SequelizeEntitiy('<%- projectName%>')
<% }
%>export<% if(createDecorators){%> class <% } else {%> interface <% } %><%- view.getMappedName('TypeScript')%> {<%
view.columns.forEach(function(column){%>
    <%- column.getMappedName('TypeScript')%><% if(column.nullable) {%>?<%}%>: <%- column.getMappedDataType('TypeScript');%>;<%
});%>
}
<%}); %><% if(namespace !== undefined && namespace !== null){ %>
} // namespace <%- namespace%>
<% } %>
